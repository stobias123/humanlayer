# Copy this file to hld-secret.yaml and fill in real values
# DO NOT commit hld-secret.yaml to version control
apiVersion: v1
kind: Secret
metadata:
  name: hld-secrets
  namespace: humanlayer
  labels:
    app.kubernetes.io/name: hld
    app.kubernetes.io/component: daemon
type: Opaque
stringData:
  # Required: HumanLayer API key
  HUMANLAYER_API_KEY: "hl_your_api_key_here"

  # Optional: Provider API keys for proxy features
  # ANTHROPIC_API_KEY: "sk-ant-..."
  # OPENROUTER_API_KEY: "sk-or-..."
  # BASETEN_API_KEY: "..."
